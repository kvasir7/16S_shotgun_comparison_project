###call qiime2 env in conda
conda activate qiime2-2022.2

###procrustes analysis with jaccard pcoa files
qiime diversity procrustes-analysis --i-reference /home/joe/Documents/NSF_Fellowship/16S_Shotgun_Comparison_Study/Qiita_Analysis/MiSeq_16S_Museum_leopard_frog_gut/Analysis_Shotgun_Woltka_Species/153728_shotgun_pcoajaccard_speciesWoL_renamed.qza --i-other /home/joe/Documents/NSF_Fellowship/16S_Shotgun_Comparison_Study/Qiita_Analysis/MiSeq_16S_Museum_leopard_frog_gut/Analysis_16S_Deblur_150bp_Filter/153719_pcoajaccard_deblurall1000_renamed.qza --output-dir Procrustes-analysis-Jaccard

###plotting the procrustes analysis output from above
qiime emperor procrustes-plot --i-reference-pcoa /home/joe/Documents/NSF_Fellowship/16S_Shotgun_Comparison_Study/Qiita_Analysis/MiSeq_16S_Museum_leopard_frog_gut/Results/Procrustes-analysis-Jaccard_153728_shotgun_pcoajaccard_speciesWoL_renamed_AND_153719_16S_pcoajaccard_deblurall1000_renamed/transformed_reference.qza --i-other-pcoa /home/joe/Documents/NSF_Fellowship/16S_Shotgun_Comparison_Study/Qiita_Analysis/MiSeq_16S_Museum_leopard_frog_gut/Results/Procrustes-analysis-Jaccard_153728_shotgun_pcoajaccard_speciesWoL_renamed_AND_153719_16S_pcoajaccard_deblurall1000_renamed/transformed_other.qza --m-metadata-file /home/joe/Documents/NSF_Fellowship/16S_Shotgun_Comparison_Study/Qiita_Analysis/MiSeq_16S_Museum_leopard_frog_gut/SampleMetadata_qiita.txt --output-dir Procrustes-plot-Jaccard
